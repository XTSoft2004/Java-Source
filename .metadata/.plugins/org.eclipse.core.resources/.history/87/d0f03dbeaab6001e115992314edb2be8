
public class InfoStudent {
	public String name;
	public String date;
	public double dtb;
	public String xeploai;
	public String gioitinh;
	
	
	public InfoStudent() {
		super();
	}
	public InfoStudent(String name, String date, double dtb, String xeploai) {
		super();
		this.name = name;
		this.date = date;
		this.dtb = dtb;
		this.xeploai = xeploai;
	}
	
	
}
