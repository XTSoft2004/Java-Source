
public class vidu2 {
	public double thuong(double a, double b) throws Exception {
		double s = a / b;
		return s;
	}
	public static void main(String[] args) {
		try {
			System.out.println(thuong(1,0));
		} catch (Exception e) {
			// TODO: handle exception
		}
		// TODO Auto-generated method stub
		
	}

}
